{
  "manifest_version": 2,
  "name": "Session ID Extractor",
  "version": "1.0",
  "permissions": [
    "cookies",
    "webNavigation",
    "activeTab",
    "storage",
    "https://*.instagram.com/*",
    "webRequest",
    "webRequestBlocking"
],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
